/* Comentario de bloco no topo deve ser ignorado */

func main() : int {
  // comentario de linha antes da primeira atribuicao
  let x : int = 1;

  // outro comentario de linha
  let y : int = x + 1;

  /* comentario de bloco
     em varias linhas
     ainda dentro da funcao
  */
  return y;
}
